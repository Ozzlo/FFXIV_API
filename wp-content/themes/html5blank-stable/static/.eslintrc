{
  "extends": "eslint-config-airbnb-es5",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "valid-jsdoc": ["error", {
      "requireReturnType": true,
      "requireReturnDescription": false,
      "requireParamDescription": false,
    }],
    "no-undef": 0,
    "func-names": 0,
    "key-spacing": 0,
    "no-extend-native": 0,
    "spaced-comment": 0,
    "no-eval": 0,
  }
}
